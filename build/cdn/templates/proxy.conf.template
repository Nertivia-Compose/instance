server {
    listen 80;
    server_name ${VUE_APP_CLIENT_SERVERNAME};
    
    location / {
        proxy_pass http://client:${VUE_APP_CLIENT_PORT};
    }
}

server {
    listen 80;
    server_name ${VUE_APP_SERVER_SERVERNAME};
    
    location / {
        proxy_pass http://server:${VUE_APP_SERVER_PORT};
    }
}